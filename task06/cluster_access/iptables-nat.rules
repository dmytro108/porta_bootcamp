-t nat -N MINIKUBE
-t nat -A MINIKUBE -i ens18 -p tcp -m tcp --dport 9090 -j DNAT --to-destination 192.168.49.2:30090
-t nat -A MINIKUBE -i ens18 -p tcp -m tcp --dport 443 -j DNAT --to-destination 192.168.49.2:30443
-t nat -A MINIKUBE -i ens18 -p tcp -m tcp --dport 80 -j DNAT --to-destination 192.168.49.2:30080
