-N MINIKUBE
-A MINIKUBE -d 192.168.49.2/32 -p tcp -m tcp --dport 30090 -j ACCEPT
-A MINIKUBE -d 192.168.49.2/32 -p tcp -m tcp --dport 30443 -j ACCEPT
-A MINIKUBE -d 192.168.49.2/32 -p tcp -m tcp --dport 30080 -j ACCEPT
-A FORWARD -d 192.168.49.2/32 -p tcp -j MINIKUBE
